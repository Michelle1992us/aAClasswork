<h1> All the cats </h1>

<h1>This is our index page</h1>

<% @cats.each do |cat| %>
<ul>

   <li><a href=<%= cat_url(cat.id) %>><%= cat.name %></a></li>
   <li><%= cat.birth_date %></li>
   <li><%= cat.color %></li>
   <li><%= cat.sex %></li>
   <li><%= cat.description %></li>
   
</ul>
    
<% end %>


